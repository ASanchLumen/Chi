- var steps = ['0', '1', '2', '3', '4', '5', '6'];
- var stepLast = 12

h2 Examples
p.-text Use the class <code>chi-pagination</code> to render a pagination component.

h3 Base
.example.-mb--3
  .chi-grid.-no-gutter
    .chi-col.-w--12.-w-md--6
      .-p--3
        nav.chi-pagination(role='navigation', aria-label='Pagination')
          .chi-pagination__content
            .chi-pagination__start
            .chi-pagination__center
              .chi-pagination__button-group.chi-button-group
                each step in steps
                  if step == '0'
                    a(class=`chi-button -icon -flat`, href='#', aria-label='Previous page')
                      .chi-button__content
                        i(class="chi-icon icon-chevron-left", aria-hidden='true')
                  else if step == '6'
                    a(class=`chi-button -icon -flat`, href='#', aria-label='Next page')
                      .chi-button__content
                        i(class="chi-icon icon-chevron-right", aria-hidden='true')
                  else
                    a(class=`chi-button -flat ${step === '3' ? `-active` : ''}`, href='#')= `${step}`
            .chi-pagination__end
    .chi-col.-w--12.-w-md--6
      .-p--3.-bg--black
        nav.chi-pagination.-inverse(role='navigation', aria-label='Pagination')
          .chi-pagination__content
            .chi-pagination__start
            .chi-pagination__center
              .chi-pagination__button-group.chi-button-group
                each step in steps
                  if step == '0'
                    a(class=`chi-button -icon -flat -light`, href='#', aria-label='Previous page')
                      .chi-button__content
                        i(class="chi-icon icon-chevron-left", aria-hidden='true')
                  else if step == '6'
                    a(class=`chi-button -icon -flat -light`, href='#', aria-label='Next page')
                      .chi-button__content
                        i(class="chi-icon icon-chevron-right", aria-hidden='true')
                  else
                    a(class=`chi-button -flat -light ${step === '3' ? `-active` : ''}`, href='#')= `${step}`
            .chi-pagination__end
  .example-tabs.-pl--2
    ul.chi-tabs#example-base
      li.-disabled
        a(href=`#webcomponent-base`) Web Component
      li.-active
        a(href=`#html-base`) HTML Blueprint
  .chi-tabs-panel#webcomponent-base
  .chi-tabs-panel.-active#html-base
    :code(lang="html")
      <!-- For light backgrounds -->
      <nav class="chi-pagination" role="navigation" aria-label="Pagination">
        <div class="chi-pagination__content">
          <div class="chi-pagination__start"></div>
          <div class="chi-pagination__center">
            <div class="chi-pagination__button-group chi-button-group">
